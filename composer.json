	"type": "mediawiki-skin",
	"description": "The Material Design language created by Google, captured in MediaWiki software",
	"keywords": [
		"MediaWiki",
		"Skin",
		"Google Material Design"
	],
	"homepage": "https://www.mediawiki.org/wiki/Skin:Material",
	"license": "GPL-3.0-or-later",
	"authors": [
		{
			"name": "Samantha Nguyen",
			"email": "samanthanguyen1116@gmail.com",
			"homepage": "https://mediawiki.org/wiki/User:SamanthaNguyen",
			"role": "Developer"
		},
		{
			"name": "Jack Phoenix",
			"email": "jack@countervandalism.net",
			"homepage": "https://mediawiki.org/wiki/User:Jack_Phoenix",
			"role": "Developer"
		},
		{
			"name": "George Barnick",
			"email": "georgebarnick@gmail.com",
			"homepage": "https://georgebarnick.photo",
			"role": "Developer"
		},
		{
			"name": "MtMNC",
			"homepage": "https://mediawiki.org/wiki/User:MtMNNC",
			"role": "Developer"
		}
	],
	"support": {
		"wiki": "https://www.mediawiki.org/wiki/Skin:Material",
		"forum": "https://www.mediawiki.org/wiki/Skin_talk:Material",
		"source": "https://github.com/SamanthaNguyen/mediawiki-skins-Material.git",
		"issues": "https://github.com/SamanthaNguyen/mediawiki-skins-Material/issues"
	},
	"require": {
		"php": ">=5.4.4",
		"param-processor/param-processor": "~1.0.0"
	},
	"config": {
		"optimize-autoloader": true,
		"prepend-autoloader": false
	},
	"require-dev": {
		"mediawiki/minus-x": "1.1.0",
		"php-parallel-lint/php-console-highlighter": "0.5.0",
		"php-parallel-lint/php-parallel-lint": "1.2.0"
	},
	"scripts": {
		"fix": [
			"minus-x fix ."
		],
		"test": [
			"parallel-lint . --exclude vendor --exclude node_modules",
			"minus-x check ."
		]
	}
}
